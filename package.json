    "jscs": "^1.4.5",
    "jshint": "^2.5.0",
    "mc-logger": "0.0.0",
    "mocha": "^1.18.2",
    "mongodb": "^1.4.28"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:domharrington/database-updates.git"
  }
}
